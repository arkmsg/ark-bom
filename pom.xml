            </dependency>
            <dependency>
                <groupId>com.whaleal.ark</groupId>
                <artifactId>ark-common-sqltemplate</artifactId>
                <version>${ark.version}</version>
            </dependency>
            <dependency>
                <groupId>com.whaleal.ark</groupId>
                <artifactId>ark-common-script</artifactId>
                <version>${ark.version}</version>
            </dependency>
            <dependency>
                <groupId>com.whaleal.ark</groupId>
                <artifactId>ark-calculation</artifactId>
                <version>${ark.version}</version>
            </dependency>
            <dependency>
                <groupId>com.whaleal.ark</groupId>
                <artifactId>ark-api-control</artifactId>
